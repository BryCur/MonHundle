{
  "ConnectionStrings": {
    "DefaultConnection": "Host={HOST_ADDRESS};Port={HOST_PORT};Database={DB_NAME};Username={DB_USER};Password={DB_PWD}"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "LogSink",
        "Args": {
          "nodeUris": "PLACEHOLDER_SINK_URI",
          "apiKey": "PLACEHOLDER_SINK_KEY",
          "indexFormat": "MonHundle-{0:yyyy.MM}"
        }
      }
    ]
  }
}
  